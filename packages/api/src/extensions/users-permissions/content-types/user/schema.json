{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "Users"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "uid": {
      "type": "customField",
      "customField": "plugin::field-uuid.uuid"
    },
    "receipts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::receipt.receipt",
      "mappedBy": "user"
    },
    "wallets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::wallet.wallet",
      "mappedBy": "user"
    },
    "transactionCategorizationRules": {
      "displayName": "Categorization Rules",
      "type": "component",
      "repeatable": true,
      "component": "transaction.categorization-rules"
    },
    "encryptionKey": {
      "type": "string"
    },
    "encryptionSalt": {
      "type": "string"
    },
    "encryptedData": {
      "type": "string"
    }
  }
}
